<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.StudentMapper">
	<!-- 根据id查询用户 parameterType 入参的类型 resultType 返回值类型 -->
	<select id="selectClassInfoExplain" parameterType="String"
		resultType="String">
		SELECT `explain` FROM classinfo WHERE NAME='T128'
	</select>
</mapper>